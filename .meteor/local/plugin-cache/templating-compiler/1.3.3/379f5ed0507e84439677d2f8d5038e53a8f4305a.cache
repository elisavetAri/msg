{"head":"","body":"","js":"\nTemplate.__checkName(\"status_bootstrap3\");\nTemplate[\"status_bootstrap3\"] = new Template(\"Template.status_bootstrap3\", (function() {\n  var view = this;\n  return Blaze.Unless(function() {\n    return Spacebars.call(view.lookup(\"connected\"));\n  }, function() {\n    return [ \"\\n    \", HTML.DIV({\n      class: function() {\n        return [ \"alert \", Spacebars.mustache(view.lookup(\"option\"), \"classes\"), \" text-center\" ];\n      },\n      role: \"alert\"\n    }, \"\\n      \", HTML.STRONG(\"\\n        \", HTML.SPAN({\n      class: \"glyphicon glyphicon-warning-sign\"\n    }), \"\\n        \", Blaze.View(\"lookup:message\", function() {\n      return Spacebars.mustache(view.lookup(\"message\"));\n    }), \"\\n      \"), \"\\n      \", Blaze.View(\"lookup:extraMessage\", function() {\n      return Spacebars.mustache(view.lookup(\"extraMessage\"));\n    }), \"\\n\\n      \", Blaze.If(function() {\n      return Spacebars.call(view.lookup(\"showReconnect\"));\n    }, function() {\n      return [ \"\\n        \", HTML.A({\n        href: \"#\",\n        class: \"alert-link\"\n      }, Blaze.View(\"lookup:reconnectLabel\", function() {\n        return Spacebars.mustache(view.lookup(\"reconnectLabel\"));\n      })), \"\\n      \" ];\n    }), \"\\n    \"), \"\\n  \" ];\n  });\n}));\n","bodyAttrs":{}}